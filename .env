# Database Credentials (Matches the default credentials used by the 'db' service)
POSTGRES_USER=agentnexus_user
POSTGRES_PASSWORD=nexus_password
POSTGRES_DB=agentnexus_db

# The URL used by the 'app' and 'worker' services to connect to the 'db' service
# Hostname 'db' resolves to the Postgres container within the Docker network
POSTGRES_URL=postgresql://agentnexus_user:nexus_password@db:5432/agentnexus_db
GEMINI_API_KEY=PLACEHOLDER_FOR_NOW
